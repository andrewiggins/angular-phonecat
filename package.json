    "license": "MIT",
    "devDependencies": {
        "@types/angular-animate": "^1.5.8",
        "@types/angular-mocks": "^1.5.10",
        "@types/angular-resource": "^1.5.9",
        "@types/angular-route": "^1.3.4",
        "@types/jasmine": "^2.5.53",
        "@types/jquery": "^2.0.47",
        "angular-loader": "^1.6.4",
        "angular-mocks": "^1.6.4",
        "http-server": "^0.9.0",
        "jasmine-core": "^2.4.1",
        "karma": "^0.13.22",
        "karma-chrome-launcher": "^0.2.3",
